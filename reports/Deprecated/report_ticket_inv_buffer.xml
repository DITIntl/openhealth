<div class="header">    
	<img src='/openhealth/static/src/img/logo_chavarri.png' style="max-height: 100px;"/>
</div>
-->



<!-- 								Page 1 - Begin	 										-->
<div class="page">    


<!-- Title --> 
<br/>
<h3>
<center>
	Ticket Factura
</center>
</h3>
<br/>



<!-- My Firm --> 
<br/>
<div class='row'>
<b>	
	<div class='col-xs-12'>		
		<t t-esc="doc.my_firm"/>
	</div>
</b>
</div>



<!-- My Address --> 
<div class='row'>
<b>	
	<div class='col-xs-12'>		
		<t t-esc="doc.my_address"/>
	</div>
</b>
</div>



<!-- My Phone --> 
<div class='row'>
	<div class='col-xs-4'>		
		<b>Telf:</b>
	</div>
	<div class='col-xs-8'>
		<t t-esc="doc.my_phone"/>
	</div>
</div>



<!-- My Ruc --> 
<div class='row'>
	<div class='col-xs-4'>		
		<b>Ruc:</b>
	</div>
	<div class='col-xs-8'>
		<t t-esc="doc.my_ruc"/>
	</div>
</div>
<br/>



<!-- Serial number --> 
<div class='row'>
	<div class='col-xs-4'>		
		<b>Serie:</b>
	</div>
	<div class='col-xs-8'>
		<t t-esc="doc.serial_nr"/>
	</div>
</div>



<!-- Authorization --> 
<div class='row'>
	<div class='col-xs-4'>		
		<b>Autorización</b>
	</div>
	<div class='col-xs-8'>
		<t t-esc="doc.authorization"/>
	</div>
</div>
<br/>





<!-- Customer --> 
<div class='row'>
	<div class='col-xs-4'>		
		<b>Cliente:</b>
	</div>
	<div class='col-xs-8'>
		<t t-esc="doc.par_name"/>
	</div>
</div>



<!-- DNI --> 
<!--
<div class='row'>
	<div class='col-xs-4'>		
		<b>DNI:</b>
	</div>
	<div class='col-xs-8'>
		<t t-esc="doc.par_dni"/>
	</div>
</div>
-->



<!-- Address --> 
<div class='row'>
	<div class='col-xs-4'>		
		<b>Dirección:</b>
	</div>
	<div class='col-xs-8'>
		<t t-esc="doc.par_address"/>
	</div>
</div>



<!-- Firm --> 
<div class='row'>
	<div class='col-xs-4'>		
		<b>Raz. Social:</b>
	</div>
	<div class='col-xs-8'>
		<t t-esc="doc.par_firm"/>
	</div>
</div>



<!-- Ruc --> 
<div class='row'>
	<div class='col-xs-4'>		
		<b>Ruc:</b>
	</div>
	<div class='col-xs-8'>
		<t t-esc="doc.par_ruc"/>
	</div>
</div>
<br/>
<br/>



<!-- Date --> 
<div class='row'>
	<div class='col-xs-4'>		
		<b>Fecha:</b>
	</div>
	<div class='col-xs-8'>
		<t t-esc="doc.date_created"/>
	</div>
</div>



<!-- Name --> 
<div class='row'>
	<div class='col-xs-4'>		
		<b>Ticket Factura:</b>
	</div>
	<div class='col-xs-8'>
		<t t-esc="doc.name"/>
	</div>
</div>
<br/>





<!-- Line --> 
<div class='row'>
	<div class='col-xs-12'>
		<t t-esc="doc.cr"/>
	</div>
</div>



<b>
<div class='row'>
	<div class='col-xs-4'>
		Descr.
	</div>
	<div class='col-xs-2'>
		Cnt
	</div>
	<div class='col-xs-3'>
		P. Unit.
	</div>
	<div class='col-xs-3'>
