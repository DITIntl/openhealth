<?xml version="1.0"?>
<openerp>
	<data>
		










		<!-- Tree view --> 
		<record model="ir.ui.view" id="base.view_user_tree">

			<field name="name">res.user.tree</field>

			<field name="model">res.users</field>

			<field name="arch" type="xml">
				


				<tree string="User Tree">
					
                    <field name="name"/>
                    <field name="login"/>
                    <field name="lang"/>
                    <field name="login_date"/>


                    <field name="groups_id"/>


                    <field name="partner_id"/>
                    <field name="active"/>


                    <field name="image"/>
				</tree>
			</field>
		</record>






		<!-- Window List action -->
		<!--
		The following tag is an action definition for a "window action",
		that is an action opening a view or a set of views
		-->

		<record model="ir.actions.act_window" id="user_list_action">
			<field name="name">Usuarios</field>
			<field name="res_model">res.users</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>

			<!--
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="view_id" eval="False"/>			
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">Create the first order
				</p>
			</field>
			-->
		</record>

	</data>
</openerp>
