<?xml version="1.0"?>
<openerp>
	<data>


		<!--
		configuration
		-->
		<record model="ir.ui.view" id="session_configuration_form_view">

			<field name="name">session.configuration.form</field>


<!--
			<field name="model">openhealth.session</field>
-->
			<field name="model">openhealth.session.med</field>



			<field name="priority" eval="1"/>
			
			<field name="inherit_id" ref="session_med_form_view"/>
			
			<field name="arch" type="xml">
				<notebook position="inside">
					
					
<!--
					<page string="Valores empleados - Med"  	attrs="{'invisible':[('cosmetology', '!=', False)]}"	>
-->


					<page string="Valores empleados - Med"  		>

						<!-- Co2 -->
						<group string="Láser Co2"  attrs="{'invisible':[('laser', '!=', 'laser_co2')]}">
							<group>
								<field name="co2_power"				required="1"/>
								<field name="vspace"/>								
								<field name="co2_frequency"			required="1"/>
								<field name="vspace"/>
								<field name="co2_energy"			required="1"/>
								<field name="vspace"/>
								<field name="co2_mode_emission"		required="1"/>
								<field name="vspace"/>
								<field name="co2_mode_exposure"		required="1"/>
								<field name="vspace"/>
								<field name="co2_observations"		required="1"/>
							</group>
						</group>
						
						


						<!-- Excilite --> 
						<!--
						<group string="Láser Eximer 308 nm">
						-->
						<group string="Láser Eximer 308 nm"   attrs="{'invisible':[('laser', '!=', 'laser_excilite')]}">
							<group>
								<field name="exc_time"			required="1"/>
								<field name="vspace"/>								
								<field name="exc_dose"			required="1"/>
								<field name="vspace"/>								
								<field name="exc_dose_selected"	required="1"/>
								<field name="vspace"/>		
								<field name="exc_dose_provided"	required="1"/>
								<field name="vspace"/>								
								<field name="exc_observations"	required="1"/>
							</group>
						</group>
						
						
						

						<!-- Ipl --> 
						<!--
						<group string="Láser M22-IPL">
						-->
						<group string="Láser M22-IPL"  attrs="{'invisible':[('laser', '!=', 'laser_ipl')]}">

							<group>
								<field name="ipl_fluency"		required="1"/>
								<field name="vspace"/>								
								<field name="ipl_phototype"		required="1"/>
								<field name="vspace"/>								
								<field name="ipl_lesion_type"	required="1"/>
								<field name="vspace"/>								
								<field name="ipl_lesion_depth"	required="1"/>
								<field name="vspace"/>								
								<field name="ipl_pulse_type"	required="1"/>
								<field name="vspace"/>								
								<field name="ipl_pulse_duration"		required="1"/>
								<field name="vspace"/>								
								<field name="ipl_pulse_time_between"	required="1"/>
								<field name="vspace"/>								
								<field name="ipl_filter"		required="1"/>
								<field name="vspace"/>								
								<field name="ipl_spot"			required="1"/>
								<field name="vspace"/>								
								<field name="ipl_observations"	required="1"/>
							</group>
						</group>
						


						
						<!-- Ndyag --> 
						<!--
						<group string="Láser M22-NDYAG">
						--> 
						<group string="Láser M22-NDYAG"  attrs="{'invisible':[('laser', '!=', 'laser_ndyag')]}">
							<group>
								<field name="ndy_fluency"		required="1"/>
								<field name="ndy_phototype"		required="1"/>
								<field name="ndy_lesion_type"	required="1"/>
								<field name="ndy_lesion_depth"	required="1"/>
								<field name="ndy_pulse_type"	required="1"/>
								<field name="ndy_pulse_duration"		required="1"/>
								<field name="ndy_pulse_time_between"	required="1"/>
								<field name="ndy_pulse_spot"	required="1"/>
								<field name="ndy_observations"	required="1"/>
							</group>
						</group>	
					</page>
				</notebook>
			</field>
		</record>
	</data>
</openerp>
