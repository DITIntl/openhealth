<?xml version="1.0"?>
<openerp>
	<data>


		<!-- Form --> 
		<record model="ir.ui.view" id="service_co2_form_view">
			<field name="name">service.co2.form</field>
			<field name="model">openhealth.service.co2</field>

			<field name="inherit_id" ref="service_form_view"/>
	        <field name="mode">primary</field>
			
			
			<field name="arch" type="xml">				
				<!-- <xpath expr="//field[@name='title']" position="before"> -->
				<xpath expr="//h1" position="replace">

					<h1>LÃ¡ser Co2</h1>					
        		
        		</xpath>

			</field>
						
		</record>		




		<!-- service tree/list view -->
		<record model="ir.ui.view" id="service_co2_tree_view">
			<field name="name">service.tree</field>
			<field name="model">openhealth.service.co2</field>
			<field name="arch" type="xml">
				
				<tree string="Service Tree">
				
					<field name="name"/>
					<field name="id"/>

					<field name="laser"/>				
					
					<field name="treatment"/>

					<field name="service"/>

					<field name="consultation"/>
				
				</tree>
			</field>
		</record>


		<!-- Window action -->
		<record model="ir.actions.act_window" id="service_co2_list_action">
			<field name="name">Servicios Co2</field>
			<field name="res_model">openhealth.service.co2</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>

		<!-- Menu -->
		<menuitem id="service_co2_menu" name="Servicios Co2"
			parent="openhealth_menu"
			action="service_co2_list_action"/>


	</data>
</openerp>
