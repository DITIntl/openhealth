<?xml version="1.0"?>
<openerp>
	<data>


		<record model="ir.ui.view" id="service_co2_zone_form_view">
			
			<field name="name">service.co2.zone.form</field>
			<field name="model">openhealth.service.co2</field>
			
			<field name="inherit_id" ref="service_co2_form_view"/>
			
			<field name="arch" type="xml">
				
				
				
				<xpath expr="//field[@name='notebook_over']" position="after">
					
				<notebook>				
				
					<page string="Zona Cuerpo">
	
						<group>						

							<group>
								<field name="co2_hands"  widget="radio" />
							</group>																									

							<group>
								<field name="co2_neck"  widget="radio" />							
							</group>
							
							<div class="jx_radio">								
								<group>
									<field name="co2_cheekbone"  widget="radio"  />							
								</group>	
							</div>	
												
							
							<group>
								<field name="co2_vagina"  widget="radio" />							
							</group>			
						</group>
						
						
						

						<hr></hr>



						<group>						
							<group>
								<field name="co2_packages"  widget="radio" />							
							</group>
						</group>
					</page>
					
					
					
					
					<page string="Todo Rostro">

						<group>
							<group>
								<field   name="co2_allface_rejuvenation"  widget="radio"/>								
							</group>
							
							<group>
								<field name="co2_allface_acnesequels"  widget="radio"/>							
							</group>							
						</group>

					</page>
					
					
										
					
					<page string="Localizado Rostro">

						<group>
							<group>
								<field name="co2_lf_scar" widget="radio"/>	
																			
							</group>
						
							<group>
								<field name="co2_lf_mole" widget="radio"/>													
							</group>
						</group>
					

						<hr></hr>


						<group>
							<group>
								<field name="co2_lf_stains" widget="radio"/>													
							</group>

							<group>
								<field name="co2_lf_keratosis" widget="radio"/>													
							</group>
						</group>
					

						<hr></hr>


						<group>
							<group>
								<field name="co2_lf_cyst" widget="radio"/>													
							</group>

							<group>
								<field name="co2_lf_wart" widget="radio"/>													
							</group>
						</group>
					
					</page>
					



					<page string="Localizado Cuerpo">
						
						
						<group>
							<group>
								<field name="co2_lb_acneseq" widget="radio"/>													
							</group>

							<group>
								<field name="co2_lb_scar" widget="radio"/>													
							</group>
						</group>

						
						



						<hr></hr>


						<group>
							<group>
								<field name="co2_lb_mole" widget="radio"/>													
							</group>

							<group>
								<field name="co2_lb_stains" widget="radio"/>													
							</group>
						</group>



						<hr></hr>

						
						
						<group col="3">
							<group>
								<field name="co2_lb_keratosis" widget="radio"/>													
							</group>

							<group>
								<field name="co2_lb_cyst" widget="radio"/>													
							</group>
							
							<group>
								<field name="co2_lb_wart" widget="radio"/>													
							</group>
						</group>
	
					</page>
					
					

				</notebook>
				</xpath>
				
				
					
			</field>
		</record>
	</data>
</openerp>
