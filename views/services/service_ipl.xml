<?xml version="1.0"?>
<openerp>
	<data>


		<!-- Form --> 
		<record model="ir.ui.view" id="service_ipl_form_view">
			<field name="name">service.ipl.form</field>
			<field name="model">openhealth.service.ipl</field>
			<field name="inherit_id" ref="service_form_view"/>
	        <field name="mode">primary</field>
			

			<field name="arch" type="xml">
				<xpath expr="//h1" position="replace">
					<h1>LÃ¡ser M22-IPL</h1>					
        		</xpath>
			</field>

		</record>		




		<!-- Tree -->
		<record model="ir.ui.view" id="service_ipl_tree_view">
			<field name="name">service.ipl.tree</field>
			<field name="model">openhealth.service.ipl</field>
			<field name="arch" type="xml">
				<tree string="Service Tree">
					<field name="name"/>
					<field name="id"/>
					<field name="laser"/>				
					<field name="treatment"/>
					<field name="service"/>
					<field name="consultation"/>
				</tree>
			</field>
		</record>




		<!-- Window action -->
		<record model="ir.actions.act_window" id="service_ipl_list_action">
			<field name="name">Servicios ipl</field>
			<field name="res_model">openhealth.service.ipl</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>

			<field name="context">{
									'default_laser': 'laser_ipl',
                    			}
			</field>

		</record>



		
	</data>
</openerp>
