<?xml version="1.0"?>
<openerp>
	<data>


		<!--
		Treatmentt - reservations
		-->
		<record model="ir.ui.view" id="treatment_reservation_form_view">
			<field name="name">treatment.reservation.form</field>
			<field name="model">openextension.treatment</field>

			
			<!--
			<field name="priority" eval="6"/>
			-->
			<field name="priority" eval="0"/>
			

			<field name="inherit_id" ref="treatment_form_view"/>
			<field name="arch" type="xml">

				
				<notebook position="inside">
					
					<page string="Reserva de salas">

						<!--
						<field name="reservation_ids"/>
						-->

						<field name="reservation_ids">

							<tree string=""		default_order="appointment_date">	

								
								<field name="name"		invisible="1"/>
								<field name="patient"	invisible="1"/>
								<field name="doctor"	invisible="1"/>


								<field name="x_type"/>

								<field name="x_machine"/>


								<field name="treatment"/>
								<field name="consultation"/>
								<field name="procedure"/>
								<field name="session"/>
								<field name="control"/>					

					
								<field name="state"/>
								<field name="x_date"/>
								<field name="x_time"/>
							</tree>

						</field>				



					</page>
				</notebook>
			</field>
		</record>
	</data>
</openerp>
