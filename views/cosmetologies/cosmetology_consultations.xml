<?xml version="1.0"?>
<openerp>
	<data>


		<!--
		consultation
		-->
		<record model="ir.ui.view" id="cosmetology_consultation_form_view">
			<field name="name">cosmetology.consultation.form</field>
			<field name="model">openhealth.cosmetology</field>

			<field name="priority" eval="4"/>
			
			<field name="inherit_id" ref="cosmetology_form_view"/>
			<field name="arch" type="xml">



				<notebook position="inside">


					<page string="Consulta">
						<!--						
						<field name="consultation_ids"/>						
						-->

						<field name="consultation_ids">
							<tree string="consultation_ids">


								<field  name="name"/>

								<field string="Paciente" name="patient"/>

								<field string="Cosmiatra" name="therapist"/>

								<field name="chief_complaint"/>

								<field name="evaluation_start_date"		string = "Fecha"/>
											


								<!-- Button -->
								<button 
									type="object" 
									class="oe_highlight"	
									string="E"
									
									name="open_line_current" 
								/>
								
							</tree>
						</field>
					</page>


				</notebook>


			</field>
		</record>
	</data>
</openerp>


