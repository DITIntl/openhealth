<?xml version="1.0"?>
<openerp>
	<data>


		<!--
		sessions
		-->
		<record model="ir.ui.view" id="cosmetology_session_form_view">
			<field name="name">cosmetology.session.form</field>
			<field name="model">openhealth.cosmetology</field>


			<field name="priority" eval="6"/>

			
			<field name="inherit_id" ref="cosmetology_form_view"/>
			<field name="arch" type="xml">
				<notebook position="inside">
					
					

					<page string="Sesiones">


<!--
						<field name="session_ids"/>
-->

						<field name="session_ids">

							<tree string="Sesiones">


								<field name="name"/>
								
								<field name="appointment"/>



								<field  name="patient"/>

								<field  name="doctor"/>



								<field name="chief_complaint"/>

								<field name="product"/>



								<field name="evaluation_start_date"/>

								<button 
									type="object" 
									class="oe_highlight"	
									string="E"
									
									name="open_line_current" 
								/>
								
							</tree>
						</field>


					</page>



				</notebook>
			</field>
		</record>
	</data>
</openerp>
