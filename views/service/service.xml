<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>



		<!--
		<record model="ir.ui.view" id="service_co2_form_view">
		-->
		<record model="ir.ui.view" id="service_form_view">
			<!--
			<field name="name">service.co2.form</field>
			<field name="model">openhealth.service.co2</field>
			-->
			<field name="name">service.form</field>
			<field name="model">openhealth.service</field>
			
			<field name="arch" type="xml">
				
				<form string="Service - Form">
					
					<sheet>
						
						<field name="title"  invisible="0" />													
											

														
							<group>
								<group>
									<field name="laser" readonly="1"  invisible="0"/>	
									
									<field name="zone" readonly="1"   invisible="0"/>
								</group>
								
								
								<group>
									<field name="consultation" readonly="1"  invisible="0"/>													

									<field name="pathology" readonly="1"  invisible="0"/>													
								</group>
								
								<!--
								<group>
									<field name="time_1" readonly="1"  invisible="0" />
								</group>
								
								<group>
									<field name="nr_sessions" readonly="1"  invisible="0" />
								</group>
								-->
							</group>

					
					
							<notebook>
							</notebook>

							<!--
							<field name="under_notebook"  invisible="0" />													
							<hr></hr>
							-->



							<div name="time">	
							</div>	


							<hr></hr>

							<group string="" >								
								<group>
									<field name="service" widget="radio"/>
								</group>
							</group>

							<!--
							<hr></hr>
							<group string="">
								<group>
									<field name="name_short" readonly="1"/>
								</group>
								<group>
									<field name="price" readonly="1"/>
								</group>
							</group>
							<hr></hr>
							-->
					</sheet>
				</form>
			</field>
		</record>





		<!-- service tree/list view -->
		<record model="ir.ui.view" id="service_co2_tree_view">
			<field name="name">service.tree</field>
			<field name="model">openhealth.service.co2</field>
			<field name="arch" type="xml">
				
				<tree string="Service Tree">
				
					<field name="name"/>
					<!--
					<field name="id"/>
					-->
					<field name="laser"/>
				
					<field name="consultation"/>
				
				</tree>
			</field>
		</record>



		<!-- Window action -->
		<record model="ir.actions.act_window" id="service_co2_list_action">
			<field name="name">Servicios Co2</field>
			<field name="res_model">openhealth.service.co2</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>


		<!-- Menu -->
		<!--
		<menuitem id="service_menu" name="Services"
		-->
		<menuitem id="service_menu" name="Servicios Co2"
			parent="openextension_menu"
			action="service_co2_list_action"/>


	</data>
</openerp>
