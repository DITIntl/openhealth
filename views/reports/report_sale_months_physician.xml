		<!-- january --> 
		<record id="action_order_report_january_phy" model="ir.actions.act_window">

			<field name="name">Reporte de Ventas - Enero</field>
			
			<field name="res_model">sale.report</field>

			<field name="view_type">form</field>
			
			<field name="view_mode">pivot,graph,tree</field>
			
			<field name="view_id"></field> 
			
			<field name="search_view_id" ref="sale.view_order_product_search"/>


		   <field name="domain">
									[

										('user_id','=','Dr. Chavarri'), 

										('date','&lt;=',time.strftime('%%Y-02-01')),
										('date','&gt;=',time.strftime('%%Y-01-01'))
									]
			</field>


			<field name="context">{
									'search_default_Sales':1, 
									'group_by_no_leaf':1,
									'group_by': ['categ_id']
								}
			</field>

			<field name="help"></field>
		</record>


