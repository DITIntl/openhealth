<?xml version="1.0"?>
<openerp>
	<data>





		<!-- Window action -->
		<record model="ir.actions.act_window" id="product_template_medical_list_action">
			<field name="name">Products</field>
			<field name="res_model">product.template</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>

			<field name="context">{'order': 'x_name_short'}</field>
            
            <field name="domain">[
									('type', '=', 'service'),
									('x_family', '=', 'medical'),
            					]</field>


		</record>

		<!-- Menu -->
		<menuitem id="product_template_medical_menu" name="Productos Medical"
			parent="openhealth_menu"
			action="product_template_medical_list_action"/>


	</data>
</openerp>
